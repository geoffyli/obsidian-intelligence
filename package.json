{
	"name": "obsidian-intelligence",
	"version": "1.0.0",
	"description": "This is a LLM agent plugin for Obsidian (https://obsidian.md)",
	"main": "main.js",
	"scripts": {
		"dev": "npm-run-all --parallel dev:*",
		"dev:tailwind": "npx tailwindcss -i src/styles/tailwind.css -o styles.css --watch",
		"dev:obsidian": "node esbuild.config.mjs",
		"build": "npm run css && node esbuild.config.mjs production",
		"css": "npx tailwindcss -i src/styles/tailwind.css -o styles.css --minify",
		"test": "jest",
		"check": "tsc -noEmit -skipLibCheck",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@types/jest": "^29.5.14",
		"@types/node": "^16.11.6",
		"@types/react": "^18.2.0",
		"@tailwindcss/container-queries": "^0.1.1",
		"@types/react-dom": "^18.2.0",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"autoprefixer": "^10.4.16",
		"builtin-modules": "3.3.0",
		"esbuild": "^0.17.3",
		"jest": "^29.7.0",
		"jest-environment-obsidian": "^0.0.1",
		"jest-extended": "^4.0.2",
		"npm-run-all": "^4.1.5",
		"obsidian": "latest",
		"postcss": "^8.4.31",
		"tailwindcss": "^3.4.15",
		"ts-jest": "^29.3.4",
		"ts-node": "^10.9.2",
		"tslib": "2.4.0",
		"typescript": "^5.8.3"
	},
	"dependencies": {
		"@langchain/community": "^0.3.43",
		"@langchain/core": "^0.3.57",
		"@langchain/openai": "^0.5.10",
		"@radix-ui/react-label": "^2.1.7",
		"@radix-ui/react-popover": "^1.1.14",
		"@radix-ui/react-select": "^2.2.5",
		"@radix-ui/react-slot": "^1.2.3",
		"@radix-ui/react-switch": "^1.2.5",
		"@radix-ui/react-tabs": "^1.1.12",
		"axios": "^1.3.4",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"dotenv": "^16.0.3",
		"langchain": "^0.3.26",
		"lucide-react": "^0.511.0",
